{
    "title": "Fahrzeugsuche",
    "content": [
        {
            "type": "group",
            "id": "zustand-auswahl",
            "label": "Fahrzeugzustand",
            "options": {
                "childs": [
                    {
                        "type": "checkbox",
                        "id": "neu",
                        "label": "Neu",
                        "options": {
                        }
                    },
                    {
                        "type": "checkbox",
                        "id": "gebraucht",
                        "label": "Gebraucht",
                        "options": {
                        }
                    }
                ]
            }
        },
        {
            "type": "group",
            "id": "modell-auswahl",
            "label": "Marke, Modell, Variante",
            "options":{
                "childs":[
                    {
                        "type": "input",
                        "id": "marke",
                        "label": "Marke",
                        "options":{
                            "inputType": "text",
                            "required": true,
                            "autocomplete": ["beliebig", "BMW", "Mercedes-Benz", "Audi", "Volkswagen", "Ford", "Tesla", "Toyota", "Porsche"]
                        }
                    },
                    {
                        "type": "input",
                        "id":"modell",
                        "label":"Modell",
                        "options":{
                            "inputType":"text",
                            "activateCond":"marke",
                            "required": false
                        }
                    },
                    {
                        "type": "input",
                        "id": "variante",
                        "label":"Variante",
                        "options": {
                            "inputType":"text",
                            "placeholder":"z. B. GTI ...",
                            "styling":"width: 100px"
                        }
                    }
                ]
            }
        },
        {
            "type":"group",
            "id":"typ-auswahl",
            "label":"Fahrzeugtyp",
            "options":{
                "childs":[
                    {
                        "type":"checkbox",
                        "id":"roadster",
                        "label":"Cabrio / Roadster",
                        "options":{}
                    },
                    {
                        "type":"checkbox",
                        "id":"kleinwagen",
                        "label":"Kleinwagen",
                        "options":{}
                    },
                    {
                        "type":"checkbox",
                        "id":"kombi",
                        "label":"Kombi",
                        "options":{}
                    },
                    {
                        "type":"checkbox",
                        "id":"limousine",
                        "label":"Limousine",
                        "options":{}
                    },
                    {
                        "type":"checkbox",
                        "id":"sportwagen",
                        "label":"Sportwagen / Coupè",
                        "options":{}
                    },
                    {
                        "type":"checkbox",
                        "id":"suv",
                        "label":"SUV / Geländewagen / Pickup",
                        "options":{}
                    },
                    {
                        "type":"checkbox",
                        "id":"van",
                        "label":"Van / Minibus",
                        "options":{}
                    },
                    {
                        "type":"checkbox",
                        "id":"andere",
                        "label":"Andere",
                        "value": true,
                        "options":{}
                    },
                    {
                        "type":"group",
                        "id":"sitzplaetze-auswahl",
                        "label":"Anzahl Sitzplätze",
                        "options":{
                            "displayCond":"van",
                            "childs":[
                                {
                                    "type":"input",
                                    "id":"sitzplatz-von",
                                    "label":"",
                                    "options":{
                                        "inputType":"number",
                                        "placeholder":"von",
                                        "autocomplete":["1", "2", "3"]
                                    }
                                },
                                {
                                    "type":"input",
                                    "id":"sitzplatz-bis",
                                    "label":"",
                                    "options":{
                                        "inputType":"number",
                                        "placeholder":"bis",
                                        "autocomplete":["4", "5", "6", "7", "8", "9"],
                                        "displayCond":"sitzplatz-von"
                                    }
                                }
                            ]
                        }
                    },
                    {
                        "type":"dropdown",
                        "id":"tueren",
                        "label":"Anzahl Türen",
                        "options":{
                            "values": ["2/3", "4/5", "6/7"],
                            "multiple":true
                        }
                    }
                ]
            }
        },
        {
            "type":"group",
            "id":"others",
            "label":"Weitere Optionen",
            "options":{
                "childs":[
                    {
                        "type":"input",
                        "id":"first-registration",
                        "label":"Erstzulassung ab",
                        "options":{
                            "inputType":"date"
                        }
                    },
                    {
                        "type":"radio",
                        "id":"engine",
                        "label":"Antriebsart",
                        "options":{
                            "pickingOptions":"api/engines"
                        }
                    }
                ]
            }
        }
    ],
    "showClearButton": false,
    "showResetButton": true
}