{
    "title": "Mein Formular",
    "content": [
        {
            "type": "group",
            "id": "a-group",
            "label": "Adresse",
            "options": {
                "childs": [
                    {
                        "type": "input",
                        "id": "text1",
                        "label": "Name",
                        "options": {
                            "inputType": "text",
                            "required": true,
                            "placeholder": "Max Mustermann",
                            "autocomplete": ["Josef", "Johannes", "Lukas", "Anna", "Maria", "Lena"]
                        }
                    },
                    {
                        "type": "input",
                        "id": "text2",
                        "label": "E-Mail",
                        "options": {
                            "value": "your@email.com",
                            "inputType": "email",
                            "required": false,
                            "activateCond": "text1"
                        }
                    }
                ],
                "activateCond": "text3"
            }
        },
        {
            "type": "input",
            "id": "text3",
            "label": "Passwort",
            "options": {
                "inputType": "password",
                "required": true,
                "placeholder": "password"
            }
        },
        {
            "type": "checkbox",
            "id": "checkbox1",
            "label": "Stadt anzeigen",
            "options": {
                "required": false
            }
        },
        {
            "type": "input",
            "id": "text4",
            "label": "Stadt",
            "options": {
                "inputType": "text",
                "required": false,
                "placeholder": "Stadt eingeben",
                "autocomplete": "api/cities",
                "displayCond": "checkbox1"
            }
        },
        {
            "type": "checkbox",
            "id": "checkbox2",
            "label": "Ich akzeptiere die allgemeinen Gesch√§ftsbedingungen",
            "options": {
                "required": true
            }
        }
    ],
    "showClearButton": true
}