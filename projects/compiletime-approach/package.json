{
    "name": "compiletime-approach",
    "version": "0.0.1",
    "scripts": {
        "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
        "copy:schemas": "rsync -R schematics/*/schema.json ../../dist/compiletime-approach/",
        "copy:files": "rm -r ../../dist/compiletime-approach/schematics/*/files/** && rsync -R -r -p schematics/*/files/** ../../dist/compiletime-approach/",
        "copy:collection": "rsync -R schematics/collection.json ../../dist/compiletime-approach/",
        "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection"
    },
    "schematics": "./schematics/collection.json",
    "ng-add": {
        "save": "devDependencies"
    },
    "peerDependencies": {
        "@angular/common": "^12.0.0",
        "@angular/core": "^12.0.0"
    },
    "dependencies": {
        "tslib": "^2.1.0"
    }
}