{
    "title": "Mein Formular",
    "content": [
        {
            "type": "group",
            "id": "a-group",
            "label": "Adresse",
            "options": {
                "childs": [
                    {
                        "type": "input",
                        "id": "text1",
                        "label": "Name",
                        "options": {
                            "inputType": "text",
                            "required": true,
                            "placeholder": "Max Mustermann"
                        }
                    },
                    {
                        "type": "input",
                        "id": "text2",
                        "label": "E-Mail",
                        "value": "meine@email.de",
                        "options": {
                            "inputType": "email",
                            "required": false,
                            "acticateCond": "text1"
                        }
                    }
                ],
                "acticateCond": "text3"
            }
        },
        {
            "type": "input",
            "id": "text3",
            "label": "Passwort",
            "options": {
                "inputType": "password",
                "required": true,
                "placeholder": "password"
            }
        },
        {
            "type": "input",
            "id": "text4",
            "label": "Stadt",
            "options": {
                "inputType": "text",
                "required": false,
                "placeholder": "Stadt eingeben",
                "autocomplete": "api/cities"
            }
        }
    ],
    "showClearButton": true
}